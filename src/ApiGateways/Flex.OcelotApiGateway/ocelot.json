{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7000",
    "RequestIdKey": "Oc-Request-ID",
    "RateLimitOptions": {
      "EnableRateLimiting": true,
      "Period": "1m",
      "Limit": 100,
      "DisableRateLimitHeaders": false
    },
    "LoadBalancerOptions": {
      "Type": "RoundRobin"
    },
    "QoSOptions": {
      "ExceptionsAllowedBeforeBreaking": 2,
      "DurationOfBreak": 1000,
      "TimeoutValue": 5000
    }
  },
  "Routes": [
    {
      "SwaggerKey": "securities-service",
      "DownstreamPathTemplate": "/api/securities/{everything}",
      "UpstreamPathTemplate": "/api/securities/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ]
    },
    {
      "SwaggerKey": "system-service",
      "DownstreamPathTemplate": "/api/system",
      "UpstreamPathTemplate": "/api/system",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7204
        }
      ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "securities-service",
      "Config": [
        {
          "Name": "Securities Api",
          "Version": "v1",
          "Url": "http://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}